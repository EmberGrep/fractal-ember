{
    "name": "embergrep/fractal-ember",
    "description": "A set of serializers for building Ember ready APIs using Fractal",
    "keywords": [
        "api",
        "ember",
        "transformer",
        "serialize"
    ],
    "homepage": "https://github.com/embergrep/fractal-ember",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Tablada",
            "email": "ryan.tablada@gmail.com",
            "homepage": "http://ryantablada.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "league/fractal": "0.11.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "EmberGrep\\Serializers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmberGrep\\Serializers\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
